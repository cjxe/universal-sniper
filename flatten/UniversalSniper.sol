        pairs = new V2Pair[](path.length-1);

        // cached variables.
        address pair;
        uint256 res0;
        uint256 res1;
        uint256 pathLen = path.length - 1;

        // iterate over path.
        for (uint i = 0; i < pathLen; i++) {
            // reset K.
